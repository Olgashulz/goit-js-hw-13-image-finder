
 
{{#each this}}
{{#each this}}
    <li class ="photo-container"> 
  
    <div class="photo-card">
    <img class ="photo-card__img" src="{{webformatURL}}" alt="{{tags}}" data-source="{{largeImageURL}}"/>
    </div>

    <div class="stats card-content">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{views}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{downloads}}
    </p>
  </div>
</div>

</li>
{{/each}}
{{/each}}


